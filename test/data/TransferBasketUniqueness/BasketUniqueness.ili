INTERLIS 2.4;

MODEL BasketUniqueness AT "mailto:geow@localhost" VERSION "2023-09-05" =

DOMAIN
                            
    SIMPLEOID = OID TEXT*2;

    TOPIC TopicA =
    BASKET OID AS SIMPLEOID;

       	CLASS ClassA =
            OID AS SIMPLEOID;
      		attr1 : MANDATORY 0..999;
            attr2 : (a,b,c);
        UNIQUE (BASKET) attr1;
        SET CONSTRAINT (BASKET) ConstraintTwo : WHERE attr2 == #b :  INTERLIS.objectCount(ALL) >= 2;
    	END ClassA;

	END TopicA;
END BasketUniqueness.
